# See https://help.github.com/articles/ignoring-files for more about ignoring files.
      #
      # Temporary files generated by your text editor or operating system
      # belong in git's global ignore instead:
      # `$XDG_CONFIG_HOME/git/ignore` or `~/.config/git/ignore`

      # Ignore bundler config.
      /.bundle

      # Ignore all environment files.
      /.env*
      !/.env.example

      # Ignore all logfiles and tempfiles.
      /log/*
      /tmp/*
      !/log/.keep
      !/tmp/.keep

      # Ignore pidfiles, but keep the directory.
      /tmp/pids/*
      !/tmp/pids/
      !/tmp/pids/.keep

      # Ignore storage (uploaded files in development and any SQLite databases).
      /storage/*
      !/storage/.keep
      /tmp/storage/*
      !/tmp/storage/
      !/tmp/storage/.keep

      # Ignore public uploads and compiled assets
      /public/assets
      /public/uploads

      # Ignore master key for decrypting credentials and more.
      /config/master.key
      /config/credentials/*.key

      # Ignore built assets from esbuild and CSS bundling
      /app/assets/builds/*
      !/app/assets/builds/.keep

      # Node modules and package manager files
      /node_modules
      /yarn-error.log
      yarn-debug.log*
      .yarn-integrity
      .pnp.*
      .yarn/*
      !.yarn/patches
      !.yarn/plugins
      !.yarn/releases
      !.yarn/sdks
      !.yarn/versions

      # Ignore esbuild output (if configured differently)
      /app/javascript/builds/*

      # IDEs and editors
      /.idea
      /.vscode
      *.swp
      *.swo
      *~
      .DS_Store

      # Ignore Rails 8 Solid Queue, Solid Cache, and Solid Cable data
      /db/queue.sqlite3
      /db/queue.sqlite3-*
      /db/cache.sqlite3
      /db/cache.sqlite3-*
      /db/cable.sqlite3
      /db/cable.sqlite3-*

      # Test coverage
      /coverage

      # Ignore local development databases (if using SQLite)
      /db/*.sqlite3
      /db/*.sqlite3-*

      # Ignore Brakeman reports
      /brakeman-report.html

      # Ignore RSpec temporary files
      /spec/examples.txt

      # Ignore Capybara screenshots
      /tmp/capybara

      # Ignore factory_bot sequences
      /spec/factories/.sequence

      # Ignore Good Job dashboard assets (if any)
      /public/good_job
config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml
